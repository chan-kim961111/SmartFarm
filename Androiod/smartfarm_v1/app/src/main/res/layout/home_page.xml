<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <ListView
        android:id="@+id/listview"
        android:layout_width="match_parent"
        android:layout_height="190dp"
        android:layout_marginTop="1dp"
        android:layout_weight="1"
        android:background="@color/White"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/constraintLayout5"
        app:layout_constraintVertical_bias="0.0" />

    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/constraintLayout2"
        android:layout_width="match_parent"
        android:layout_height="320dp"
        android:background="@color/White"
        android:elevation="1dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">

        <ImageView
            android:id="@+id/weather0"
            android:layout_width="180dp"
            android:layout_height="180dp"
            android:layout_marginTop="20dp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <TextView
            android:id="@+id/today0"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            app:layout_constraintEnd_toEndOf="@+id/weather0"
            app:layout_constraintStart_toStartOf="@+id/weather0"
            app:layout_constraintTop_toBottomOf="@+id/weather0" />

        <TextView
            android:id="@+id/today1"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textSize="12sp"
            app:layout_constraintEnd_toEndOf="@+id/weather1"
            app:layout_constraintStart_toStartOf="@+id/weather1"
            app:layout_constraintTop_toBottomOf="@+id/weather1" />

        <ImageView
            android:id="@+id/weather1"
            android:layout_width="40dp"
            android:layout_height="40dp"
            android:layout_marginEnd="10dp"
            app:layout_constraintBottom_toBottomOf="@+id/weather2"
            app:layout_constraintEnd_toStartOf="@+id/weather2"
            app:layout_constraintTop_toTopOf="@+id/weather2" />

        <ImageView
            android:id="@+id/weather2"
            android:layout_width="40dp"
            android:layout_height="40dp"
            android:layout_marginEnd="10dp"
            app:layout_constraintBottom_toBottomOf="@+id/weather3"
            app:layout_constraintEnd_toStartOf="@+id/weather3"
            app:layout_constraintTop_toTopOf="@+id/weather3" />

        <ImageView
            android:id="@+id/weather3"
            android:layout_width="40dp"
            android:layout_height="40dp"
            android:layout_marginTop="30dp"
            android:layout_marginEnd="60dp"
            app:layout_constraintEnd_toEndOf="@+id/today0"
            app:layout_constraintHorizontal_bias="0.37"
            app:layout_constraintStart_toStartOf="@+id/today0"
            app:layout_constraintTop_toBottomOf="@+id/today0" />

        <ImageView
            android:id="@+id/weather4"
            android:layout_width="40dp"
            android:layout_height="40dp"
            android:layout_marginStart="10dp"
            app:layout_constraintBottom_toBottomOf="@+id/weather3"
            app:layout_constraintStart_toEndOf="@+id/weather3"
            app:layout_constraintTop_toTopOf="@+id/weather3" />

        <ImageView
            android:id="@+id/weather5"
            android:layout_width="40dp"
            android:layout_height="40dp"
            android:layout_marginStart="10dp"
            app:layout_constraintBottom_toBottomOf="@+id/weather4"
            app:layout_constraintStart_toEndOf="@+id/weather4"
            app:layout_constraintTop_toTopOf="@+id/weather4" />

        <ImageView
            android:id="@+id/weather6"
            android:layout_width="40dp"
            android:layout_height="40dp"
            android:layout_marginStart="10dp"
            app:layout_constraintBottom_toBottomOf="@+id/weather5"
            app:layout_constraintStart_toEndOf="@+id/weather5"
            app:layout_constraintTop_toTopOf="@+id/weather5" />

        <TextView
            android:id="@+id/today2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textSize="12sp"
            app:layout_constraintEnd_toEndOf="@+id/weather2"
            app:layout_constraintStart_toStartOf="@+id/weather2"
            app:layout_constraintTop_toBottomOf="@+id/weather2" />

        <TextView
            android:id="@+id/today3"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textSize="12sp"
            app:layout_constraintEnd_toEndOf="@+id/weather3"
            app:layout_constraintStart_toStartOf="@+id/weather3"
            app:layout_constraintTop_toBottomOf="@+id/weather3" />

        <TextView
            android:id="@+id/today4"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textSize="12sp"
            app:layout_constraintEnd_toEndOf="@+id/weather4"
            app:layout_constraintStart_toStartOf="@+id/weather4"
            app:layout_constraintTop_toBottomOf="@+id/weather4" />

        <TextView
            android:id="@+id/today5"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textSize="12sp"
            app:layout_constraintEnd_toEndOf="@+id/weather5"
            app:layout_constraintStart_toStartOf="@+id/weather5"
            app:layout_constraintTop_toBottomOf="@+id/weather5" />

        <TextView
            android:id="@+id/today6"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textSize="12sp"
            app:layout_constraintEnd_toEndOf="@+id/weather6"
            app:layout_constraintStart_toStartOf="@+id/weather6"
            app:layout_constraintTop_toBottomOf="@+id/weather6" />

        <TextView
            android:id="@+id/show_date"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            app:layout_constraintBottom_toBottomOf="@+id/weather0"
            app:layout_constraintEnd_toStartOf="@+id/weather0"
            app:layout_constraintTop_toTopOf="@+id/weather0"
            app:layout_constraintVertical_bias="0.0" />

        <TextView
            android:id="@+id/day1"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textSize="12sp"
            app:layout_constraintBottom_toTopOf="@+id/weather1"
            app:layout_constraintEnd_toEndOf="@+id/weather1"
            app:layout_constraintStart_toStartOf="@+id/weather1" />

        <TextView
            android:id="@+id/day2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textSize="12sp"
            app:layout_constraintBottom_toTopOf="@+id/weather2"
            app:layout_constraintEnd_toEndOf="@+id/weather2"
            app:layout_constraintStart_toStartOf="@+id/weather2" />

        <TextView
            android:id="@+id/day3"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textSize="12sp"
            app:layout_constraintBottom_toTopOf="@+id/weather3"
            app:layout_constraintEnd_toEndOf="@+id/weather3"
            app:layout_constraintStart_toStartOf="@+id/weather3" />

        <TextView
            android:id="@+id/day4"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textSize="12sp"
            app:layout_constraintBottom_toTopOf="@+id/weather4"
            app:layout_constraintEnd_toEndOf="@+id/weather4"
            app:layout_constraintStart_toStartOf="@+id/weather4" />

        <TextView
            android:id="@+id/day5"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textSize="12sp"
            app:layout_constraintBottom_toTopOf="@+id/weather5"
            app:layout_constraintEnd_toEndOf="@+id/weather5"
            app:layout_constraintStart_toStartOf="@+id/weather5" />

        <TextView
            android:id="@+id/day6"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textSize="12sp"
            app:layout_constraintBottom_toTopOf="@+id/weather6"
            app:layout_constraintEnd_toEndOf="@+id/weather6"
            app:layout_constraintStart_toStartOf="@+id/weather6" />

    </androidx.constraintlayout.widget.ConstraintLayout>

    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/horizontalScrollView"
        android:layout_width="match_parent"
        android:layout_height="120dp"
        android:layout_marginTop="1dp"
        android:background="@color/White"
        android:elevation="1dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/constraintLayout2"/>

    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/constraintLayout5"
        android:layout_width="match_parent"
        android:layout_height="30dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/horizontalScrollView"
        android:elevation="1dp"
        android:background="@color/White">

        <ImageButton
            android:id="@+id/cabbage_bt"
            android:layout_width="72dp"
            android:layout_height="40dp"
            android:background="@color/White"
            android:elevation="1dp"
            android:scaleType="centerInside"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:srcCompat="@drawable/cabbage_unclick" />

        <ImageButton
            android:id="@+id/rice_bt"
            android:layout_width="72dp"
            android:layout_height="40dp"
            android:background="@color/White"
            android:elevation="1dp"
            android:scaleType="centerInside"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintStart_toEndOf="@+id/cabbage_bt"
            app:layout_constraintTop_toTopOf="parent"
            app:srcCompat="@drawable/rice_unclick" />

        <ImageButton
            android:id="@+id/beans_bt"
            android:layout_width="72dp"
            android:layout_height="40dp"
            android:background="@color/White"
            android:elevation="1dp"
            android:scaleType="centerInside"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintStart_toEndOf="@+id/rice_bt"
            app:layout_constraintTop_toTopOf="parent"
            app:srcCompat="@drawable/beans_unclick" />

        <ImageButton

            android:id="@+id/chili_bt"
            android:layout_width="72dp"
            android:layout_height="40dp"
            android:background="@color/White"
            android:elevation="1dp"
            android:scaleType="centerInside"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintStart_toEndOf="@+id/beans_bt"
            app:layout_constraintTop_toTopOf="parent"
            app:srcCompat="@drawable/chili_unclick" />

        <ImageButton
            android:id="@+id/strawberry_bt"
            android:layout_width="72dp"
            android:layout_height="40dp"
            android:background="@color/White"
            android:elevation="1dp"
            android:scaleType="centerInside"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintStart_toEndOf="@+id/chili_bt"
            app:layout_constraintTop_toTopOf="parent"
            app:srcCompat="@drawable/strawberry_unclick" />
    </androidx.constraintlayout.widget.ConstraintLayout>

</androidx.constraintlayout.widget.ConstraintLayout>